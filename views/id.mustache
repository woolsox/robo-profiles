<!doctype html>
<html>
 <head>
  <meta charset="utf-8" />
  <title>Robo Profiles</title>
  <link rel="shortcut icon" href="robot.ico" />
  <link rel="stylesheet" type="text/css" href="css/style.css" />
  <link rel="stylesheet" type="text/css" href="/css/style.css" />
 </head>
 <body>
   <div id="profile">
     {{#id}}
     <div class="header">
       <img src="{{avatar}}" />
       <h1>{{name}}</h1>
       <p>
         {{address.city}}, {{address.country}} | {{job}}{{^job}}Available for hire{{/job}}, {{company}} {{email}} | {{phone}}
       </p>
       {{#university}}
       Education:
       <li>
        {{university}}
       </li>
       {{/university}}
       {{^university}}
       {{/university}}
       Skills:
       {{#skills}}
       <li>
         {{.}}
       </li>
       {{/skills}}
       {{^skills}}
       {{/skills}}
     </div>
     {{/id}}
   </div>
 </body>
</html>
